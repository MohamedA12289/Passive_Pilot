{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["error", {
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_"
    }],
    "react/no-unescaped-entities": ["error", {
      "forbid": [">", "}"]
    }]
  },
  "ignorePatterns": [
    "**/*.BACKUP_*.tsx",
    "**/*.BACKUP_*.ts",
    "**/*.BEFORE_*.tsx",
    "**/*.EXPERIMENTAL.tsx",
    "**/*.REBUILD.tsx",
    "**/*.WORKING_BACKUP.tsx"
  ]
}
